<process-monitor>
    <!-- Fill out as many execute elements as you would like monitored
         be filtered out of the reported metrics. (OPTIONAL)
         Example Linux  : <execute>
                              <name>My Process</name>
                              <command>my_process.sh</command>
                          </execute>
    -->
    <process>
        <!-- The name of the execute that will be displayed on the as the metric node -->
        <name></name>
        <!-- The command to monitor, note that this can be a regular expression:
             Linux Example: /opt/vertica/bin/vertica -C DTJK0Q[0-5] .* node000[0-5]_catalog -h .*
             Windows Example: chrome.exe
         -->
        <command></command>
    </process>

</process-monitor>
